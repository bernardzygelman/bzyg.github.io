Cell[TextData[{
 "For plotting purposes, we fix initial conditions for the laser parameters \
and geometry. The incoming wavenumber is given by ",
 Cell[BoxData[
  FormBox[
   RowBox[{"k", ","}], TraditionalForm]]],
 " the incident wave amplitude ",
 Cell[BoxData[
  FormBox["Ein", TraditionalForm]]],
 ", and ",
 Cell[BoxData[
  FormBox["k2", TraditionalForm]]],
 " is the wavenumber inside the reflective surfaces (modeled by an imaginary \
value), ",
 Cell[BoxData[
  FormBox["t", TraditionalForm]]],
 " is the thickness of the reflecting material, "
}], "Text",
 CellFrame->{{3, 0}, {0, 0.5}},
 CellChangeTimes->{{3.7376397857107162`*^9, 3.737640014594234*^9}, 
   3.759501281689595*^9, {3.759501439683956*^9, 3.7595015034704437`*^9}, {
   3.7595017300355387`*^9, 3.759501736538042*^9}}],

Cell[BoxData[{
 RowBox[{
  RowBox[{"Ein", "=", "1"}], ";", 
  RowBox[{"k", "=", "2"}], ";", 
  RowBox[{"k2", "=", 
   RowBox[{"100", " ", "I"}]}], ";", 
  RowBox[{"k3", "=", 
   RowBox[{"100", " ", "I"}]}], ";", 
  RowBox[{"z1", "=", "0"}], ";", 
  RowBox[{"t", "=", "0.01"}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"wave", "[", 
   RowBox[{"z_", ",", "d1_"}], "]"}], ":=", 
  RowBox[{"Piecewise", "[", 
   RowBox[{"{", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
       RowBox[{"Re", "[", 
        RowBox[{"f1", "[", 
         RowBox[{"z", ",", "d1"}], "]"}], "]"}], ",", 
       RowBox[{"z", "<=", "z1"}]}], "}"}], ",", 
     RowBox[{"{", 
      RowBox[{
       RowBox[{"Re", "[", 
        RowBox[{"f2", "[", 
         RowBox[{"z", ",", "d1"}], "]"}], "]"}], ",", 
       RowBox[{"z1", "<", "z", "<=", " ", 
        RowBox[{"z1", "+", "t"}]}]}], "}"}], ",", 
     RowBox[{"{", 
      RowBox[{
       RowBox[{"Re", "[", 
        RowBox[{"f3", "[", 
         RowBox[{"z", ",", "d1"}], "]"}], "]"}], ",", 
       RowBox[{
        RowBox[{"z1", "+", "t"}], "<", "z", "<", 
        RowBox[{"z1", "+", "t", "+", "d1"}]}]}], "}"}], ",", 
     RowBox[{"{", 
      RowBox[{
       RowBox[{"Re", "[", 
        RowBox[{"f4", "[", 
         RowBox[{"z", ",", "d1"}], "]"}], "]"}], ",", 
       RowBox[{
        RowBox[{"z1", "+", "t", "+", "d1"}], "<", "z", "<", 
        RowBox[{"z1", "+", "d1", "+", 
         RowBox[{"2", "t"}]}]}]}], "}"}], ",", 
     RowBox[{"{", 
      RowBox[{
       RowBox[{"Re", "[", 
        RowBox[{"f5", "[", 
         RowBox[{"z", ",", "d1"}], "]"}], "]"}], ",", 
       RowBox[{"z", ">", 
        RowBox[{"z1", "+", "d1", "+", 
         RowBox[{"2", " ", "t"}]}]}]}], "}"}]}], "}"}], "]"}]}]}], "Input",
